{
  "compilerOptions": {
    "strict": false,
    "baseUrl": "src",
    "outDir": "./dist",
    "checkJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "module": "ESNext",
    "target": "ES5",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "paths": {
      "@Page/*": ["./pages/*"],
      "@Component/*": ["./components/*"],
      "@Hook/*": ["./hooks/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
